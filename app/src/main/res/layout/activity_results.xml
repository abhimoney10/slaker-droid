<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="slaker.sydneyuni.au.com.slaker.activities.ResultsActivity"
    android:id="@+id/activity_results">

    <TextView
        android:id="@+id/activity_message_CoefA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/activity_message_CoefB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_message_CoefA"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/activity_message_CoefC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_message_CoefB"
        android:layout_alignParentStart="true" />

</RelativeLayout>
